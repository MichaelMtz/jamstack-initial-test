        </script>
        <script type="application/ld+json">{"@context":"http://schema.org","@type":"Organization","name":"SnoCountry","legalName":"SnoCountry, Inc","url":"https://www.snocountry.com/","logo":"https://www.snocountry.com/images/snocountry_logo_v2.png","foundingDate":"1968","sameAs":["https://www.facebook.com/SnoCountryMountainReports/","https://twitter.com/SnoCountryCom","https://www.youtube.com/channel/UCyWmeXdZjJYZ-VafL9ijq5g"]}
        </script>
        
        <link rel="stylesheet" href="/assets/font/mulish-font.css">
        <link rel="stylesheet" href="/assets/css/snocountry-base.css">
        <link rel="stylesheet" href="/assets/css/sno-core.css">
        <link rel="stylesheet" href="/assets/css/sno-modal.css">

        {% if resorts.styles %}
          {% for cssFileName in resorts.styles %}
            <link rel="stylesheet" href="/assets/css/{{cssFileName}}">
          {% endfor %}
         {% endif %}
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
{% include 'google.html' %}
    </head>
    <body data-source="{{ resorts.entryType }}" data-snowreport="{{ resorts.snowreport }}"" data-resorttype="{{ resorts.resortType }}">
        

{% include 'nav-header.html' %}
{{ content | safe }}
  <div id="sno-modal" class="modal">
    <div class="modal-container">
      <div class="modal-left">
        <div class="input-container">
          <h1 class="modal-title">Join the SnoCountry Community!</h1>
          <p class="modal-desc">The latest reports and snow sports delivered directly to you!</p>
        
          <div class="input-block">
            <label for="firtName" class="input-label">First Name</label>
            <input type="text" name="firstName" id="firstName" placeholder="First Name">
          </div>
          <div class="input-block">
            <label for="lastName" class="input-label">Last Name</label>
            <input type="text" name="lastName" id="lastName" placeholder="Last Name">
          </div>
          <div class="input-block">
            <label for="email" class="input-label">Email</label>
            <input type="email" name="email" id="email" placeholder="Email">
          </div>
          <div class="input-block">
            <label for="state" class="input-label">State</label>
            <input type="text" name="state" id="state" placeholder="State">
          </div>
          
          <div class="modal-buttons">
            <button id="modal-submit" class="input-button">Send It!</button>
          </div>
        </div>
        <div class="signup-result">
          <h1 class="modal-title">You nailed the landing!</h1>
          <p id="result-message" class="modal-desc">Look in your inbox for the latest Sno News. <br/>  <small>(Modal will close shortly)</small></p>
          <div class="social">
          
               <a href="https://www.facebook.com/SnoCountryMountainReports/" target="_blank" rel="noopener noreferrer">
                  <img class=" jch-lazyloaded" src="/assets/images/core/facebook.png" data-src="/assets/images/core/facebook.png" alt="facebook">
                  <noscript><img src="/assets/images/core/facebook.png" alt="facebook" /></noscript></a> 
            <a href="https://twitter.com/SnoCountryCom" target="_blank" rel="noopener noreferrer">
               <img class=" jch-lazyloaded" src="/assets/images/core/twitter.png" data-src="/assets/images/core/twitter.png" alt="twitter">
               <noscript><img src="/assets/images/core/twitter.png" alt="twitter" /></noscript>
            </a>
            <a href="https://www.instagram.com/Sno_Cntry/">
               <img class=" jch-lazyloaded" src="/assets/images/core/instagram.png" data-src="/assets/images/core/instagram.png" alt="instagram">
               <noscript><img src="/assets/images/core/instagram.png" alt="instagram" /></noscript>
            </a>
            <a href="https://www.youtube.com/channel/UCyWmeXdZjJYZ-VafL9ijq5g" target="_blank" rel="noopener noreferrer">
               <img class=" jch-lazyloaded" src="/assets/images/core/youtube.png" data-src="/assets/images/core/youtube.png" alt="youtube">
               <noscript><img src="/assets/images/core/youtube.png" alt="youtube" /></noscript>
            </a>
          </div>
        </div>
      </div>
      <div class="modal-right">
        <img src="http://snocountry.com/assets/images/sign-up/sign-up.png" alt="">
      </div>
      <button class="icon-button close-button">
